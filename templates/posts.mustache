<h1>Posts
  <a href="/feed.atom" class="float-right">
    <span class="fa fa-rss"></span>
  </a>
</h1>

<table class="table table-striped">
  <thead class="thead-default">
    <tr>
      <th>Published</th>
      <th>Title</th>
    </tr>
  </thead>
  {{# post }}
  <tr>
    <td>{{ published }}</td>
    <td><a href="{{ file }}">{{ title }}</a></td>
  </tr>
  {{/ post }}
</table>
